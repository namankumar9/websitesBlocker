{
  "manifest_version": 3,
  "name": "Text Storage Extension",
  "version": "1.0",
  "description": "An extension that stores and displays entered text.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ]
}
